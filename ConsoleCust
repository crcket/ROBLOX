-- handler vv

local logService = game:GetService("LogService")
local frameHolder = ScrollingFrameHolder
local TopBar = frameHolder.TopBar

local scrollingFrame = frameHolder.ScrollingFrame
local closedIcon = "rbxassetid://75740071997600"
local openIcon = "rbxassetid://108968904216785"
local goDownIcon = "rbxassetid://101777562519389"
local trashCan = TopBar.Trashcan
local scrollDownBtn = TopBar.ScrollDownBTN
local codeRunTXT = frameHolder.runCodeLine

local function makeDraggable(frame)
	local dragging = false
	local dragInput
	local startPos
	local startMousePos
	local userInputService = game:GetService("UserInputService")
	local function onDragStart(input)
		dragging = true
		startPos = frame.Position
		startMousePos = input.Position
	end
	local function onDragMove(input)
		if dragging then
			local delta = input.Position - startMousePos
			frame.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
		end
	end
	local function onDragEnd()
		dragging = false
	end
	frame.InputBegan:Connect(function(input, gameProcessed)
		if gameProcessed then return end
		if input.UserInputType == Enum.UserInputType.MouseButton1 then
			onDragStart(input)
		end
	end)
	userInputService.InputChanged:Connect(function(input)
		if dragging and input.UserInputType == Enum.UserInputType.MouseMovement then
			onDragMove(input)
		end
	end)
	frame.InputEnded:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 then
			onDragEnd()
		end
	end)
end
makeDraggable(MainThingyIdkBypass)
codeRunTXT.FocusLost:Connect(function(enterPressed:boolean)
	if enterPressed then
		loadstring(codeRunTXT)()
	end
end)


trashCan.MouseButton1Click:Connect(function()
	scrollingFrame.TextLabel.Text = ""
	scrollingFrame.CanvasSize = UDim2.fromOffset(0, 0) 
end)
scrollDownBtn.MouseButton1Click:Connect(function()
	scrollingFrame.CanvasPosition = Vector2.new(0, scrollingFrame.TextLabel.TextBounds.Y)
end)

logService.MessageOut:Connect(function(msg, messageType)
	if messageType == Enum.MessageType.MessageInfo then
		msg = "<font color='#A2C2FF'>" .. msg .. "</font>"
	elseif messageType == Enum.MessageType.MessageError then
		msg = "<font color='#FFB3B3'>" .. msg .. "</font>"
	elseif messageType == Enum.MessageType.MessageOutput then
		msg = "<font color='#B3FFB3'>" .. msg .. "</font>"
	elseif messageType == Enum.MessageType.MessageWarning then
		msg = "<font color='#FFFFB3'>" .. msg .. "</font>"
	end
	msg = "> " .. msg
	if scrollingFrame.TextLabel.Text == "" then
		scrollingFrame.TextLabel.Text = msg 
	else
		scrollingFrame.TextLabel.Text = scrollingFrame.TextLabel.Text .. "\n" .. msg
	end
	scrollingFrame.CanvasSize = UDim2.fromOffset(0, scrollingFrame.TextLabel.TextBounds.Y)
end)
